<?xml version="1.0" encoding="utf-8" ?>
<Layout2D xmlns="http://waher.se/Layout/Layout2D.xsd"
          background="WhiteBackground" pen="BlackPen"
          font="Text" textColor="Black">
  <SolidPen id="BlackPen" color="Black" width="1px"/>
  <SolidPen id="LightGrayPen" color="LightGray" width="1px"/>
  <SolidPen id="RedPen" color="Red" width="2mm"/>
  <SolidBackground id="WhiteBackground" color="WhiteSmoke"/>
  <SolidBackground id="RedBackground" color="{Alpha('Red',128)}"/>
  <Font id="Text" name="Arial" size="20pt" color="White"/>
  <Grid columns="8">
    <Cell colSpan="7" border="BlackPen"/>
    <Cell border="BlackPen">
      <Margins left="1mm" top="1mm" bottom="1mm" right="1mm">
        <RoundedRectangle radiusX="5mm" radiusY="5mm" height="2cm" fill="RedBackground">
          <Margins left="0.5em" right="0.5em">
            <Label text="Edge Services" x="50%" y="50%" halign="Center" valign="Center"/>
          </Margins>
        </RoundedRectangle>
      </Margins>
    </Cell>

    <Cell colSpan="6" border="BlackPen"/>
