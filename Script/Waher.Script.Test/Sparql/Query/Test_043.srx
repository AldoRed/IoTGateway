<?xml version="1.0"?>
<sparql xmlns="http://www.w3.org/2005/sparql-results#"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3.org/2001/sw/DataAccess/rf1/result2.xsd">

  <head>
		<variable name="mbox"/>
		<variable name="nick"/>
		<variable name="ppd"/>
	</head>

  <results>
		<result>
			<binding name="mbox">
				<uri>mailto:bob@work.example</uri>
			</binding>
			<binding name="nick">
				<literal>Robert</literal>
			</binding>
			<binding name="ppd">
				<uri>http://example.org/foaf/bobFoaf</uri>
			</binding>
		</result>
	</results>

</sparql>
