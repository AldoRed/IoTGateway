<?xml version="1.0"?>
<sparql xmlns="http://www.w3.org/2005/sparql-results#"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3.org/2001/sw/DataAccess/rf1/result2.xsd">

  <head>
		<variable name="name"/>
		<variable name="mbox"/>
		<variable name="date"/>
	</head>

  <results>
		<result>
			<binding name="name">
				<literal>Bob</literal>
			</binding>
			<binding name="mbox">
				<uri>mailto:bob@oldcorp.example.org</uri>
			</binding>
			<binding name="date">
				<literal datatype="http://www.w3.org/2001/XMLSchema#date">2004-12-06</literal>
			</binding>
		</result>
		<result>
			<binding name="name">
				<literal>Bob</literal>
			</binding>
			<binding name="mbox">
				<uri>mailto:bob@newcorp.example.org</uri>
			</binding>
			<binding name="date">
				<literal datatype="http://www.w3.org/2001/XMLSchema#date">2005-01-10</literal>
			</binding>
		</result>
	</results>

</sparql>
